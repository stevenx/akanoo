<plugin name='akanoo-core' version='0.1-SNAPSHOT' grailsVersion='2.1.0 &gt; *'>
  <author>Fabian Gebert</author>
  <authorEmail>fabian@akanoo.com</authorEmail>
  <title>Akanoo Core Plugin</title>
  <description>Collaborative note taking app</description>
  <documentation>http://grails.org/plugin/akanoo-core</documentation>
  <type>AkanooCoreGrailsPlugin</type>
  <resources>
    <resource>com.akanoo.AkanooController</resource>
    <resource>com.akanoo.Board</resource>
    <resource>com.akanoo.Note</resource>
    <resource>com.akanoo.AtmosphereService</resource>
    <resource>com.akanoo.ClientService</resource>
    <resource>com.akanoo.UserServiceInterface</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='org.grails' name='grails-plugin-testing' version='2.1.0' />
    </test>
    <runtime>
      <dependency group='com.google.gwt' name='gwt-servlet' version='2.4.0' />
      <dependency group='mysql' name='mysql-connector-java' version='5.1.18' />
      <dependency group='org.eclipse.jdt.core.compiler' name='ecj' version='3.6.2' />
    </runtime>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='tomcat' version='2.1.0' />
      <plugin group='com.akanoo' name='akanoo-core' version='0.1-SNAPSHOT' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='webxml' version='1.4.1' />
      <plugin group='org.grails.plugins' name='cached-resources' version='1.0' />
      <plugin group='org.grails.plugins' name='gpars' version='0.3' />
      <plugin group='org.grails.plugins' name='oauth' version='2.0.3' />
      <plugin group='org.grails.plugins' name='facebook-sdk' version='0.3.4' />
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2.7.3' />
      <plugin group='org.grails.plugins' name='lesscss-resources' version='1.3.0.3' />
      <plugin group='org.grails.plugins' name='mail' version='1.0' />
      <plugin group='org.grails.plugins' name='rest-client-builder' version='1.0.2' />
      <plugin group='org.grails.plugins' name='zipped-resources' version='1.0' />
      <plugin group='org.grails.plugins' name='spring-security-acl' version='1.1.1' />
      <plugin group='org.grails.plugins' name='resources' version='1.1.6' />
      <plugin group='org.grails.plugins' name='jquery' version='1.7.2' />
      <plugin group='org.grails.plugins' name='cache-headers' version='1.1.5' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.1.0' />
      <plugin group='org.grails.plugins' name='gpars' version='0.3' />
    </runtime>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>